 CREATE DATABASE QUANLYDEAN
 USE QUANLYDEAN 
 CREATE TABLE CHUYENMON (
 MaTDCM char (3) PRIMARY KEY NOT NULL,
   Tenchuyenmon nvarchar(100),
   );
   
insert into CHUYENMON values ('A01', N'KĨ SƯ Công nghệ thông tin')

